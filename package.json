{
  "name": "@nativescript-community/ui-parallax",
  "version": "0.0.1",
  "description": "NativeScript Parallax View Plugin.",
  "main": "nativescript-parallax.js",
  "private": false,
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo &&  cd demo &&  ns run ios",
    "demo.android": "npm run preparedemo &&  cd demo &&  ns run android",
    "preparedemo": "npm run build &&  cd demo &&  tns plugin remove nativescript-parallax &&  ns plugin add .. &&  ns install",
    "setup": "npm install && cd demo &&  npm install &&  cd .. &&  npm run build &&  cd demo &&  ns plugin add .. &&  cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nativescript-community/ui-parallax.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS",
    "Parallax"
  ],
  "author": {
    "name": "Mayer Lench",
    "email": "mayerlench1314@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/nativescript-community/ui-parallax/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/nativescript-community/ui-parallax",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "@nativescript/core": "~8.3.0"
  },
  "devDependencies": {
    "@nativescript/android": "8.3.1",
    "@nativescript/core": "~8.3.0",
    "@nativescript/ios": "8.3.3",
    "@nativescript/webpack": "~5.0.9",
    "react-nativescript": "^4.0.0",
    "typescript": "~4.5.5"
  }
}
